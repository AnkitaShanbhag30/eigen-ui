{
  "name": "eigen-ui",
  "private": true,
  "packageManager": "pnpm@8.15.0",
  "type": "module",
  "engines": { "node": ">=18.17" },
  "scripts": {
    "dyad:dev": "cd tools/dyad && pnpm dev",
    "dyad:sync": "node scripts/dyad-sync.mjs",
    "ssr": "node scripts/ssr-render.mjs",
    "test:dyad": "python tests/test_dyad_integration.py",
    "test:comprehensive": "node scripts/run-tests.mjs --full",
    "test:quick": "node scripts/run-tests.mjs --quick",
    "test:report": "node scripts/run-tests.mjs --report-only",
    "evaluate:single": "node scripts/gpt-asset-evaluator.mjs evaluate",
    "evaluate:batch": "node scripts/gpt-asset-evaluator.mjs batch",
    "evaluate:report": "node scripts/gpt-asset-evaluator.mjs report",
    "v0:gen": "node scripts/v0-generate.mjs"
