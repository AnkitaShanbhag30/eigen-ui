  "devDependencies": {
    "chokidar": "^3.5.3",
    "esbuild": "^0.19.8",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "v0-sdk": "^0.7.1",
    "dotenv": "^16.4.5"
